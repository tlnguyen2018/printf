#ifndef _HOLBERTON_H_
#define _HOLBERTON_H_
#include <unistd.h>
int _putchar(char c);
int _printf(const char *format, ...);

/**
 *_prtstr - entry point
 *@s: pointer to the string
 *Description: print string with putchar
 */
void _prtstr(char *s)
{
	while (*s)
	{
		_putchar(*s);
		s++;
	}
}

/**
 *_print_int - entry point
 *@n: interger
 *Description: print string with putchar
 */
void _print_int(int n)
{
	if (n < 0)
	{
		_putchar('-');
		n = -n;
	}
	if (n / 10 != 0)
		_print_int(n / 10);
	_putchar((n % 10) + '0');
}

#endif
